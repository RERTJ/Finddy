        </div>
<!-- searchresult -->
        <div ng-show="detail.length">
        <div class='searchMain'>
    		<div class="container s-results margin-bottom-50">
    			<h4><span class="results-number">We found <span ng-bind="detail.length"></span> activities for your: </span></h4>
    			<!-- Begin Inner Results -->
    			<div class="inner-results" ng-repeat="i in detail">
    				<a ng-href="/activity/{{i.AID}}">
          <h4>With {{i.USERNAME}} @ {{i.LOCATION}}</h4>
          </a>
    				</h4>
    				<p>
    					User <span ng-bind="i.USERNAME"></span> invites you to join his activity at <span ng-bind="i.START_TIME"></span>.<br>
              Description: <span ng-bind="i.DESCRIPTION"></span> <br>
              It's such a hit that <span ng-bind="i.NO_OF_JOINERS"></span> out of <span ng-bind="i.QUOTA"></span> quota have been processed.
    				</p>
    			</div>
    			<!-- Begin Inner Results -->

    			<hr>


    			<!-- Begin Inner Results -->

    		</div>
    	</div>
    </div>

